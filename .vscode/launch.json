{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "{MakGitDevFork}/src",
        },
        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "{MakGitDevFork}/src",
        },
        {
            "name": "Launch Firefox",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "{MakGitDevFork}/src",
        },
        {
            "name": "Launch Safari",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "{MakGitDevFork}/src",
        },
        {
            "name": "Launch Edge (Chromium)",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:8080",
            "webRoot": "{MakGitDevFork}/src",
        },
        {
            "name": "Launch Chrome (Incognito)",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "{MakGitDevFork}/src",
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal", // or "integrated terminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
            "<node_internals>/**"
            ],
            "program": "${file}"
        }
    ],
}